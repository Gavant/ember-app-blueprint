{
    "name": "<%= name %>",
    "version": "0.0.0",
    "private": true,
    "description": "<%= name %> Ember.js application",
    "repository": "",
    "author": "Gavant Software, Inc.",
    "directories": {
        "doc": "doc",
        "test": "tests"
    },
    "scripts": {
        "build": "graphql-codegen --config codegen.yml && ember build --environment=production",
        "lint": "npm-run-all --aggregate-output --continue-on-error --parallel \"lint:!(fix)\"",
        "lint:fix": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*:fix",
        "lint:hbs": "ember-template-lint .",
        "lint:hbs:fix": "ember-template-lint . --fix",
        "lint:js": "eslint . --cache",
        "lint:js:fix": "eslint . --fix",
        "start": "concurrently -k --names \"codegen,ember\"  \"yarn graphql-codegen-watch\" \"ember serve\"",
        "test": "npm-run-all lint test:*",
        "test:ember": "ember test",
        "graphql-codegen": "graphql-codegen --config codegen.yml -r dotenv/config",
        "graphql-codegen-watch": "graphql-codegen --watch \"app/graphql/**/*.ts\" --config codegen.yml -r dotenv/config",
        "postinstall": "yarn run graphql-codegen"
    },
